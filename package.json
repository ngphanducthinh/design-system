    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-storybook": "^9.0.17",
    "eslint-plugin-vue": "^10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "storybook": "^9.0.17",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@^7.0.9",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.3"
